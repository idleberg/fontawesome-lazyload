{
  "name": "fontawesome-svg-loader",
  "version": "0.3.2",
  "description": "Loads Font Awesome SVG icons and inlines them in place of the default markup",
  "license": "MIT",
  "main": "dist/fontawesome-svg-loader.js",
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/fontawesome-svg-loader.git"
  },
  "keywords": [
    "fontawesome",
    "font-awesome",
    "svg",
    "async"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "dev": "npm run start",
    "lint": "tslint --project .",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "webpack --mode none --watch",
    "test": "npm run lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^14.0.1",
    "babel-loader": "^8.1.0",
    "babel-minify": "^0.5.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "babel-plugin-minify-simplify": "^0.5.1",
    "css-loader": "^3.5.3",
    "husky": "^4.2.5",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": "> 0.25%, IE 11, not dead"
}
